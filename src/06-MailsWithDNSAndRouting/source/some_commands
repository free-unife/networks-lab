# Reload running-config in router

# Add default gateway
sudo ip route add default via 192.168.2.1

# sudo es3/final/start_all.sh

# Addresses:
1001::1/127 S0
1002::5/127 S1
2002::1/64 ETH

# Per ogni interfaccia
ipv6 ospf 23 area 12

show ip route
show ipv6 route

